a = np.arange(1,13,1)
a

a.reshape(2,6)

a.reshape(3,4)

a.reshape(4,3)

#operations on array
a = np.array([1,2,3,4,5])
a

a = np.array([1, 2, 3, 4]).reshape(2, 2)
a


#unique

a = np.array([1, 2, 2, 2, 2, 2, 2, 2, 1, 4, 5])
np.unique(a, return_index=True, return_counts=True)




a = np.linspace(2,3,5)
a

#hstack and vstack

a = np.array([1,2,3,4])
b = np.array([5,6,7,8])
c = np.array([9,10,11,12])

np.hstack((a,b,c))

np.vstack((a,b,c))
